{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
        "title": {
            "type": "string",
            "minLength": 1,
            "maxLength": 60
        },
        "salary": {
            "type": "integer",
            "minimum": 0
        },
        "equity": {
            "type": "integer",
            "minimum": 0
        },
        "company_handle": {
            "type": "string",
            "minLength": 1,
            "maxLength": 60
        }
    },
    "additionalProperties": false,
    "required": [ 
        "title",
        "salary",
        "equity",
        "company_handle"
    ]
}